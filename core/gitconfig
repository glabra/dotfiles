[core]
	excludesfile = ~/.config/git/gitignore
[push]
	default = nothing
[color]
	ui = true
[alias]
	a = add
	b = branch
	d = diff
	l = log --graph --date=iso --stat
	s = status --branch --short
	co = commit
	ch = checkout
	po = push origin
	lo = log --graph --date=iso --pretty=format:'%C(auto)%h: by %aN%n%s%n'
