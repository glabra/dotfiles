# rust
source_if_exists_lazy "${HOME}/.cargo/env" \
    rustc rustdoc rust-gdb rust-lldb rustup cargo

# nvm
export NVM_DIR="$HOME/.nvm"
source_if_exists_lazy "$NVM_DIR/nvm.sh" \
    nvm npm node

# android-studio
append_path_if_exists "${HOME}/.local/android-studio/bin"
append_path_if_exists "${HOME}/Android/Sdk/build-tools/25.0.1"
append_path_if_exists "${HOME}/Android/Sdk/platform-tools"
