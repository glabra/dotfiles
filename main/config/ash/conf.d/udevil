smb_mount_ts () {
        udevil mount smb://berochoro@192.168.1.3/ts
}

smb_unmount_ts () {
        udevil unmount smb://berochoro@192.168.1.3/ts
}

smb_mount_berochoro () {
        udevil mount smb://berochoro@192.168.1.3/berochoro
}

smb_unmount_berochoro () {
        udevil unmount smb://berochoro@192.168.1.3/berochoro
}

